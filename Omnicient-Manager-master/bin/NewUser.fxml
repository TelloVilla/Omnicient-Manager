<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="400.0">
         <center>
            <GridPane maxHeight="211.0" maxWidth="385.0" prefHeight="171.0" prefWidth="273.0" BorderPane.alignment="CENTER">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="271.0" minWidth="10.0" prefWidth="271.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <PasswordField fx:id="NewPassword" onAction="#handleNewPassword" prefHeight="28.0" prefWidth="220.0" GridPane.rowIndex="3" />
                  <Label text="Enter New Password" GridPane.rowIndex="2" />
                  <Label prefHeight="18.0" prefWidth="152.0" text="Enter New Username" />
                  <TextField fx:id="NewUsername" onAction="#handleNewUsername" GridPane.rowIndex="1" />
               </children>
            </GridPane>
         </center>
         <top>
            <Button fx:id="ReturnButton" mnemonicParsing="false" onAction="#handleReturn" text="Back" translateX="-40.0" translateY="30.0" BorderPane.alignment="CENTER_RIGHT" />
         </top>
         <bottom>
            <Button fx:id="ConfirmButton" mnemonicParsing="false" onAction="#handleConfirmButton" text="Confirm" translateX="100.0" translateY="-50.0" BorderPane.alignment="CENTER" />
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
